{
    "name": "nanosoup/mimir",
    "license": "Unlicense",
    "description": "skeleton for wp dev",
    "type": "composer-plugin",
    "authors": [
        {
            "name": "Jez Emery",
            "email": "jez@hiohzo.com"
        },
        {
            "name": "Harry Wiseman",
            "email": "enquiries@harry-wiseman.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:strawberrysoup/nemesis.git"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "symfony/dotenv": "^4.2@dev",
        "strawberrysoup/nemesis": "dev-master",
        "twig/twig": "2.x-dev",
        "johnpbloch/wordpress": "dev-master",
        "wp-cli/wp-cli": "^2.0@dev"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "scripts": {
        "post-create-project-cmd": "mv -n wordpress/* . && rm -rf wordpress"
    }
}
